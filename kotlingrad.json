{
  "properties": {
    "kotlingradVersion": "0.2.6",
    "kmathVersion": "0.1.4-dev8",
    "graphvizVersion": "0.17.0",
    "viktorVersion":"1.0.1"
  },
  "link": "https://github.com/breandan/kotlingrad",
  "repositories": [
    "https://jitpack.io",
    "https://dl.bintray.com/kotlin/kotlin-eap",
    "https://dl.bintray.com/mipt-npm/dev",
    "https://dl.bintray.com/hotkeytlt/maven"
  ],
  "dependencies": [
    "com.github.breandan:kotlingrad:$kotlingradVersion",
    "scientifik:kmath-ast:$kmathVersion",
    "scientifik:kmath-core:$kmathVersion",
    "scientifik:kmath-prob:$kmathVersion",
    "org.jetbrains.bio:viktor:$viktorVersion",
    "guru.nidi:graphviz-kotlin:$graphvizVersion"
  ],
  "imports": [
    "edu.umontreal.kotlingrad.experimental.*"
  ],
  "renderers": {
    "edu.umontreal.kotlingrad.experimental.SFun": "HTML($it.html())",
    "edu.umontreal.kotlingrad.experimental.VFun": "HTML($it.html())",
    "edu.umontreal.kotlingrad.experimental.MFun": "HTML($it.html())"
  }
}
