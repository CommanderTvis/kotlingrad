{
  "properties": {
    "kotlingradVersion": "0.3.2"
  },
  "link": "https://github.com/breandan/kotlingrad",
  "repositories": [
    "https://jitpack.io"
  ],
  "dependencies": [
    "com.github.breandan:kotlingrad:$kotlingradVersion"
  ],
  "imports": [
    "edu.umontreal.kotlingrad.experimental.*"
  ],
  "renderers": {
    "edu.umontreal.kotlingrad.experimental.SFun": "HTML(($it as SFun<*>).html())",
    "edu.umontreal.kotlingrad.experimental.VFun": "HTML(($it as VFun<*>).html())",
    "edu.umontreal.kotlingrad.experimental.MFun": "HTML(($it as MFun<*>).html())"
  }
}